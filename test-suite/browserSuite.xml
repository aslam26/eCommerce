<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="eCommerce Website Test">
    <test name="Chrome Test" enabled="true">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.luma.Tests.LoginTest"/>
        </classes>
    </test><!-- Test -->
    <test name="edge test" enabled="true">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.luma.Tests.LoginTest"/>
        </classes>
    </test>
    <test name="Chromeheadless Test">
        <parameter name="browser" value="chromeHeadless"/>
        <classes>
            <class name="com.luma.Tests.LoginTest"/>
        </classes>
    </test>
</suite> <!-- Suite -->
